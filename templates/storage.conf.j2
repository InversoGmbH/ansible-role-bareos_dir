{{ ansible_managed | comment }}

Storage {
  Name = "{{ item.name }}"
  Address = "{{ item.address }}"
  Password = "{{ item.password }}"
  Device = "{{ item.device }}"
  Media Type = "{{ item.media_type }}"
  TLS Enable = {{ item.tls_enable | ternary('Yes', 'No') }}
  TLS Verify Peer = {{ item.tls_verify_peer | ternary('Yes', 'No') }}
}
